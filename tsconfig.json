    "compilerOptions": {
        "outDir": "target/",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "es5",
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "es2020",
        "importHelpers": true,
        "jsx": "react",
        "downlevelIteration": true,
        "lib": ["DOM", "WebWorker"]
    }
}
